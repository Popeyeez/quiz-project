generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model articles {
  id         Int       @id @default(autoincrement())
  title      String
  content    String
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
